{
  "name": "celebnetwork-backend",
  "version": "1.0.0",
  "description": "CelebNetwork Backend API",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "nodemon --exec ts-node src/main.ts",
    "start:prod": "node dist/main.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d src/database/data-source.ts",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "deploy": "serverless deploy"
  },
  "keywords": [
    "nest",
    "api",
    "celebrity",
    "network"
  ],
  "author": "CelebNetwork Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/swagger": "^8.0.5",
    "@types/passport-jwt": "^4.0.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25",
    "openai": "^4.73.1",
    "puppeteer": "^23.8.0",
    "axios": "^1.7.9",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^24.0.8",
    "@types/aws-lambda": "^8.10.145",
    "@types/aws-serverless-express": "^3.3.5",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.18.0",
    "nodemon": "^3.1.10",
    "serverless": "^4.4.7",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-offline": "^14.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
